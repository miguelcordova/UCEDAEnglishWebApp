						<script type="text/javascript">
						//<![CDATA[
						$(document).ready(function (){
							$("#searchbutton").bind("click", function (event) {
										$.ajax({
											beforeSend: function (XMLHttpRequest) {
														maskId = $.mask({parent: '.search', text:'Searching...'});}, 
											data:$("#searchbutton").closest("form").serialize(), 
											success:function (data, textStatus) {
														$.unmask({ id: maskId, 
																   callback: function() { 
																				objSearch.callback(data); 
																			  }
														});
													}, 
											type:"post", 
											url:"https:\/\/demo.openemis.org\/core\/Students"});
						
										return false;
							});
						});
						//]]>
						</script>
								
